{
  "name": "boilterplate-webpack-eslint-prettier-editorconfig",
  "version": "1.0.0",
  "description": "Minimal boilerplate to get start project with Webpack (Node), Eslint, Prettier, Editorconfig",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/bundle.js",
    "dev": "npx webpack --silent true --watch --config webpack.config.js",
    "build": "npx webpack --config webpack.config.js"
  },
  "keywords": [
    "webpack",
    "eslint",
    "prettier",
    "editorconfig",
    "babel",
    "babel-preset-env"
  ],
  "author": "Subrata Mal <06.subrata@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv-webpack": "^1.5.7",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.4",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "nodemon-webpack-plugin": "^4.0.3",
    "prettier": "^1.13.7",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  }
}
